      <h4 style="text-align: center">Label</h4>
      <span class="text-muted" style="margin-left: 60px;">Something else</span>
    </div>
  </div>
  <h1 style="color:#333;margin-top: 50px;">List of Students</h1>
  <hr/>

  <div class="table-responsive">
  <table class="table-striped tablee" cellpadding="3" style="width: 910px;color: #333333;margin-bottom: 50px">
    <thead>
    <tr>
      <th class="col-md-1">#</th>
      <th class="col-md-2">Student Name</th>
      <th class="col-md-2">Status</th>
      <th class="col-md-2">Actions</th>
    </tr>
    </thead>
    <% @applicants.each do |applicant| %>
        <% personal_detail = applicant.personal_detail %>
        <% if personal_detail.present? %>
            <tr>
              <td><%= applicant.try(:id) %></td>
              <td><%= "#{personal_detail.first_name} #{personal_detail.middle_name} #{personal_detail.last_name}" %></td>
              <td><%= OnlineApplicationForm.get_status(applicant.status).capitalize %></td>
              <td><%= link_to('View', online_application_form_path(applicant.id), :class => 'btn btn-info no-print', style: 'margin-right:3px;') %><button class="btn btn-warning" style="margin-right: 3px;">Edit</button><button class="btn btn-danger" style="margin-right: 3px;">Delete</button></td>
            </tr>
        <% end %>
    <% end %>
  </table>
    </div>
</div>