<h1>Listing language_abilities</h1>

<table>
  <tr>
    <th>Applicant</th>
    <th>Name</th>
    <th>Level</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @language_abilities.each do |language_ability| %>
  <tr>
    <td><%= language_ability.online_application_form_id %></td>
    <td><%= language_ability.name %></td>
    <td><%= language_ability.level %></td>
    <td><%= link_to 'Show', language_ability %></td>
    <td><%= link_to 'Edit', edit_language_ability_path(language_ability) %></td>
    <td><%= link_to 'Destroy', language_ability, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

