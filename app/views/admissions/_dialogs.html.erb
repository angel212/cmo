
<div id="preferences_dialog" title="My Preferences" style="overflow-y: auto;">
<span>
  <form id="preference-details-form" class="form-horizontal" method="post" action="/admissions/submit/<%= @online_application.id %>">
    <table style="padding-left: 50px;color: #7d7d7d;width: 565;margin: 0 auto 0 auto;font-family: helvetica, verdana, arial, sans-serif;font-size: 12px;" cellpadding="3">
      <tr>
        <td>Industry - Preference:</td>
        <td>
          <% @online_application.preference_industries.each do |preference_industry| %>
              <span id="pre_industry<%= preference_industry.id %>" class="language-container" ><%= preference_industry.industry %> - <%= preference_industry.sub_industry %><button  id="<%= preference_industry.id %>" class="preference-industry-delete">delete</button><br></span>

          <% end %>
        </td>
      </tr>
      <tr>
        <td>Most Important Criteria for Choosing a Job:</td>
        <td>
          <% @online_application.preference_criteriums.each do |preference_criteriums| %>
              <span id="pre_criteria<%= preference_criteriums.id %>" class="language-container" ><%= preference_criteriums.criteria %> <button  id="<%= preference_criteriums.id %>" class="preference-criteria-delete">delete</button><br></span>

          <% end %>
        </td>
      </tr>
      <tr>
        <td>Desired job titles:</td>
        <td>
          <input id="" name="preference[job_title]" value="<%= @preference.job_title %>" type="text" class="input-xlarge">
        </td>
      </tr>
      <tr>
        <td>Desired Salary after MBA (per annum):</td>
        <td>

          <select id="salary_after_mba" name="preference[salary_after_mba]" class="input-xlarge dropdown-value" data-chosen-value="<%= @preference.salary_after_mba %>">

            <option>Below $10,000</option>
            <option>$10,000 - $14,999</option>
            <option>$15,000 - $19,999</option>
            <option>$20,000 - $24,999</option>
            <option>$25,000 - $29,999</option>
            <option>$30,000 - $34,999</option>
            <option>$35,000 - $39,999</option>
            <option>$40,000 - $44,999</option>
            <option>$45,000 - $49,999</option>
            <option>$50,000 above</option>


          </select>

        </td>
      </tr>
      <tr>
        <td>Candidate Job Reporting Status:</td>
        <td>
          <input id="" name="preference[candidate_job_status]" value="<%= @preference.candidate_job_status %>" type="text" class="input-xlarge">
        </td>
      </tr>
    </table>

  </form>


<span>
