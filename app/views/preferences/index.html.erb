<h1>Listing preferences</h1>

<table>
  <tr>
    <th>Industry</th>
    <th>Criteria</th>
    <th>Job title</th>
    <th>Salary after mba</th>
    <th>Candidate job status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @preferences.each do |preference| %>
  <tr>
    <td><%= preference.industry %></td>
    <td><%= preference.criteria %></td>
    <td><%= preference.job_title %></td>
    <td><%= preference.salary_after_mba %></td>
    <td><%= preference.candidate_job_status %></td>
    <td><%= link_to 'Show', preference %></td>
    <td><%= link_to 'Edit', edit_preference_path(preference) %></td>
    <td><%= link_to 'Destroy', preference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Preference', new_preference_path %>
